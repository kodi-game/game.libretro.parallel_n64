<?xml version="1.0" encoding="UTF-8"?>
<addon id="game.libretro.parallel_n64"
		name="Nintendo - Nintendo 64 (ParaLLEl N64)"
		version="0.2.0.29"
		provider-name="Hacktarux, Mupen64Plus Team, TinyTiger, Libretro">
	<requires>
		<import addon="game.libretro" version="1.0.0"/>
	</requires>
	<extension point="kodi.gameclient"
			library_@PLATFORM@="@LIBRARY_FILENAME@">
		<platforms></platforms>
		<extensions>n64|v64|z64|bin|u1|ndd</extensions>
		<supports_vfs>true</supports_vfs>
		<supports_standalone>false</supports_standalone>
		<requires_opengl>true</requires_opengl>
	</extension>
	<extension point="xbmc.addon.metadata">
		<license>GPLv2</license>
		<platform>@PLATFORM@</platform>
		<source>https://github.com/kodi-game/game.libretro.parallel_n64</source>
		<assets>
			<icon>resources/icon.png</icon>
		</assets>
		<summary lang="en_GB">Nintendo - Nintendo 64 (ParaLLEl N64)</summary>
		<summary lang="es_ES">Nintendo - Nintendo 64 (ParaLLEl N64)</summary>
		<description lang="da_DK">ParaLLEl N64 er en optimeret/omskrevet Nintendo 64-emulator baseret på Mupen64 Plus. Den blev lavet specielt til Libretro.</description>
		<description lang="de_DE">ParaLLEl N64 ist ein optimierter/umgeschriebener Nintendo-64-Emulator, der auf Mupen64 Plus basiert. Er wurde speziell für Libretro entwickelt.</description>
		<description lang="en_GB">ParaLLEl N64 is an optimized/rewritten Nintendo 64 emulator based on Mupen64 Plus. It was made specifically for Libretro.</description>
		<description lang="es_ES">ParaLLEl N64 es un emulador optimizado/reescrito de Nintendo 64 basado en Mupen64 Plus. Se hizo específicamente para Libretro.</description>
		<description lang="it_IT">ParaLLEl N64 è un emulatore Nintendo 64 ottimizzato/riscritto basato su Mupen64 Plus. È stato realizzato appositamente per Libretro.</description>
		<description lang="zh_CN">ParaLLEl N64 是一款基于 Mupen64 Plus 优化/重写的任天堂64模拟器。它是专门为 Libretro 而开发的。</description>
	</extension>
</addon>
